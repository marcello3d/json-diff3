  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcello3d/json-diff3.git"
  },
  "keywords": [
    "json",
    "three-way-merge",
    "diff",
    "diff3",
    "merge"
  ],
  "author": {
    "name": "Marcello Bast√©a-Forte",
    "email": "marcello@cellosoft.com"
  },
  "license": "Zlib",
  "bugs": {
    "url": "https://github.com/marcello3d/json-diff3/issues"
  },
  "homepage": "https://github.com/marcello3d/json-diff3#readme",
  "dependencies": {
    "node-diff3": "1.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "codecov": "^3.5.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "rollup": "^1.13.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-typescript2": "^0.21.1",
    "ts-jest": "24.0.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
